{
    "variable": {
        "proj": {
            "default": "dev",
            "//": "name which will be used to tag/name the resources accordingly"
        },
        "eks_version": {
            "default": "1.28",
            "//": "EKS version to use"
        },
        "cidr_block": {
            "default": "10.0.0.0/16",
            "//": "vpc cidr range"
        },
        "instance_tenancy": {
            "default": "default",
            "//": "tenancy"
        },
        "enable_dns_hostnames": {
            "default": true,
            "//": "enable_dns_hostnames"
        },
        "enable_dns_support": {
            "default": true,
            "//": "enable_dns_support"
        },
        "map_public_ip_on_launch": {
            "default": [
                false,
                false
            ],
            "//": "isPublicIpOnLaunch"
        },
        "az": {
            "default": [
                "us-east-1a",
                "us-east-1b"
            ],
            "//": "AZ list where subnets will be deployed"
        },
        "subnets": {
            "default": [
                "10.0.128.0/23",
                "10.0.144.0/23"
            ],
            "//": "subents cidr range"
        },
        "tags": {
            "default": [
                "eks-1a",
                "eks-1b"
            ],
            "//": "subents cidr range"
        },
        "gateway_id": {
            "default": "local",
            "//": "gateway ids"
        },
        "vpc_endpoints": {
            "default": [
                "com.amazonaws.us-east-1.ec2",
                "com.amazonaws.us-east-1.ecr.api",
                "com.amazonaws.us-east-1.ecr.dkr",
                "com.amazonaws.us-east-1.eks",
                "com.amazonaws.us-east-1.s3"
            ],
            "//": "list of vpc endpoints required"
        },
        "vpc_endpoints_tags": {
            "default": [
                "ec2",
                "ecr.api",
                "ecr.dkr",
                "eks",
                "s3"
            ],
            "//": "list of vpc endpoints tags"
        },
        "max_size": {
            "default": 1,
            "//": "max size of node"
        },
        "min_size": {
            "default": 1,
            "//":  "max size of node"
        },
        "desired_size": {
            "default": 1,
            "//": "desired size of the node"
        },
        "iam_roles": {
            "default": [],
            "//": "list of vpc endpoints tags"
        }
    }
}